{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"strict": true,
		"lib": ["ES2022", "DOM"],
		"types": ["@jlceda/pro-api-types"],
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/extension/**/*"]
}
